#. module: mgmtsystem_action
#: model:mail.template,body_html:mgmtsystem_action.action_email_template_reminder_action
msgid ""
"\n"
"                \n"
"    <p>Hello,</p>\n"
"\n"
"    <p>\n"
"      This is an automated message to remind you that the\n"
"      following action is due to be completed in 10 days (${object.date_deadline}):\n"
"\n"
"      <a href=\"${object.get_action_url()}\">${object.get_action_url()}</a>\n"
"    </p>\n"
"\n"
"    <p>\n"
"      Thank you and have a nice day.<br />\n"
"      --<br />\n"
"      Odoo\n"
"    </p>\n"
"            \n"
"            "
msgstr ""
"\n"
"                \n"
"    <P> Olá, </ p>\n"
"\n"
"     <P>\n"
"       Esta é uma mensagem automática para lembrá-lo de que o\n"
"       após a acção deverá ser concluída em 10 dias ($ {object.date_deadline}):\n"
"\n"
"       <a href=\"${object.get_action_url()}\"> $ {object.get_action_url ()} </a>\n"
"     </ P>\n"
"\n"
"     <P>\n"
"       Obrigado e tenha um bom dia. <br />\n"
"       - <br />\n"
"       Openerp\n"
"     </ P>\n"
"            "

#. module: mgmtsystem_action
#: model:mail.template,body_html:mgmtsystem_action.email_template_new_action_reminder
msgid ""
"\n"
"                \n"
"<p>Hello,</p>\n"
"<p>A new action has been assigned to you : <a href=\"${object.get_action_url()}\">${object.get_action_url()}</a></p>\n"
"<p>${object.description}</p>\n"
"\n"
"<p>\n"
"  Thank you<br />\n"
"  --<br />\n"
"  Odoo\n"
"</p>\n"
"            "
msgstr ""
"\n"
"                \n"
"<P> Olá, </ p>\n"
"<P> Um novo recurso foi atribuído a você: <a href=\"${object.get_action_url()}\"> $ {object.get_action_url ()} </a> </ p>\n"
"<P> $ {object.description} </ p>\n"
"\n"
"<P>\n"
"   Obrigado <br />\n"
"   - <br />\n"
"   Openerp\n"
"</ P>\n"
"\n"
"            "

